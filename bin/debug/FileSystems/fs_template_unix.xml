<FCFileSystem owner={FSOwner} id={FSOwnerID} style="unix" casesensitive="0">
	<Command name="ver" action="OSVersion" />
	<Command name="cd" action="ChangeDir" />
	<Command name="ls" action="FileListing" />
	<File type="dir" name="bin">
		<File type="file" name="ver" mutable="0" />
		<File type="file" name="ping" mutable="0" action="ConnectionCheck" />
		<File type="file" name="telnet" mutable="0" action="RemoteConnection" />
		<File type="file" name="ls" mutable="0" />
		<File type="file" name="ftp" mutable="0" action="RemoteFile" />
	</File>
	<File type="dir" name="etc">
		<File type="file" name="passwd" mutable="0" />
		<File type="file" name="hosts" mutable="0" />
	</File>
	<File type="dir" name="users">
		<File type="dir" name={CharacterName}>
			<File type="dir" name="Downloads">
			</File>
			<File type="dir" name="Docs">
			</File>
			<File type="dir" name="Scripts">
			</File>
		</File>
	</File>
	<File type="dir" name="logs" />
</FCFileSystem>